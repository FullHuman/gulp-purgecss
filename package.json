{
  "name": "gulp-purgecss",
  "version": "1.2.0",
  "description": "Gulp plugin for purgecss",
  "main": "./lib/gulpPurgecss.js",
  "module": "./lib/gulpPurgecss.es.js",
  "jsnext:main": "./lib/gulpPurgecss.es.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__",
    "example": "example"
  },
  "files": [
